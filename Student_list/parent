using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace student_list
{
    class Parent
    {
        string name;
        string cellNum;
        Parent(string name , string cellNum)
        {
            this.name = name;
            this.cellNum = cellNum;

           
        }

        public string get_name() { return name; }
        public string get_cellNum() { return cellNum; }

        public void set_name(string name) { this.name = name; }
        public void set_cellNum(string cellNum) { this.cellNum = cellNum; }



    }
}
